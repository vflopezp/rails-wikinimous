<h1>New aritcle</h1>
<%= form_for (@article) do |f| -%>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class:"form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :content %>
    <%= f.text_area :content, class:"form-control" %>
  </div>
  <%= f.submit "Create article", class:"btn btn-outline-dark" %>
<% end %>
<%= link_to "Back", articles_path %>
